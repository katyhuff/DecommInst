<simulation>
  <control>
    <duration>1200</duration>
    <startmonth>1</startmonth>
    <startyear>2014</startyear>
  </control>

  <archetypes>
    <spec>
      <lib>cycamore</lib>
      <name>Source</name>
    </spec>
    <spec>
      <lib>cycamore</lib>
      <name>EnrichmentFacility</name>
    </spec>
    <spec>
      <lib>cycamore</lib>
      <name>GrowthRegion</name>
    </spec>
    <spec>
      <path>decomm</path>
      <lib>DecommInst</lib>
      <name>DecommInst</name>
    </spec>
    <spec>
      <path>streamblender</path>
      <lib>StreamblenderFacility</lib>
      <name>StreamblenderFacility</name>
    </spec>
    <spec>
      <path>commodconverter</path>
      <lib>CommodconverterFacility</lib>
      <name>CommodconverterFacility</name>
    </spec>
  </archetypes>

  <facility>
    <name>Mine</name>
    <config>
      <Source>
        <out_commod>nat_u</out_commod>
        <recipe_name>nat_u_recipe</recipe_name>
        <capacity>200000</capacity>
      </Source>
    </config>
  </facility>

  <facility>
    <name>Enrichment</name>
    <config>
      <EnrichmentFacility>
        <in_commod>nat_u</in_commod>
        <in_recipe>nat_u_recipe</in_recipe>
        <out_commod>leu</out_commod>
        <tails_assay>0.0025</tails_assay>
      </EnrichmentFacility>
    </config>
  </facility>

  <facility>
    <name>LWRFuelFab</name>
    <config>
      <CommodconverterFacility>
        <in_commod>leu</in_commod>
        <in_recipe>leu_recipe</in_recipe>
        <out_commod>fresh_lwr_fuel</out_commod>
        <out_recipe>fresh_lwr_fuel_recipe</out_recipe>
        <process_time>12</process_time>
        <max_inv_size>100.0</max_inv_size>
      </CommodconverterFacility>
    </config>
  </facility>

  <facility>
    <name>SFRFuelFab</name>
    <config>
      <StreamblenderFacility>
        <in_commods>
          <val>rep_sfr_tru</val>
          <val>rep_lwr_tru</val>
          <val>rep_sfr_u</val>
          <val>nat_u</val>
          <val>dep_u</val>
        </in_commods>
        <out_commod>fresh_sfr_fuel</out_commod>
        <out_recipe>fresh_sfr_fuel_recipe</out_recipe>
        <in_recipes>
          <val>rep_sfr_tru_recipe</val>
          <val>rep_lwr_tru_recipe</val>
          <val>rep_sfr_u_recipe</val>
          <val>nat_u_recipe</val>
          <val>dep_u_recipe</val>
        </in_recipes>
        <preflist>
          <prefiso>94240</prefiso>
          <sourcecommod>rep_sfr_tru</sourcecommod>
          <sourcecommod>rep_lwr_tru</sourcecommod>
        </preflist>
        <preflist>
          <prefiso>92235</prefiso>
          <sourcecommod>rep_sfr_u</sourcecommod>
          <sourcecommod>dep_u</sourcecommod>
          <sourcecommod>nat_u</sourcecommod>
        </preflist>
        <processtime>12</processtime>
        <capacity>100.0</capacity>
        <commodity_production>
          <commodity>fresh_sfr_fuel</commodity>
          <capacity>10000.0</capacity>
          <cost>10000.0</cost>
        </commodity_production>
      </StreamblenderFacility>
    </config>
    <incommodity>rep_sfr_tru</incommodity>
    <incommodity>rep_lwr_tru</incommodity>
    <incommodity>rep_sfr_u</incommodity>
    <incommodity>nat_u</incommodity>
    <incommodity>dep_u</incommodity>
    <outcommodity>fresh_sfr_fuel</outcommodity>
  </facility>

  <facility>
    <name>LWR</name>
    <lifetime>1</lifetime>
    <config>
      <BatchReactor>
        <fuel>
         <incommodity>fresh_lwr_fuel</incommodity>
         <inrecipe>fresh_lwr_fuel_recipe</inrecipe>
         <outcommodity>lwr_unf</outcommodity>
         <outrecipe>lwr_unf_recipe</outrecipe>
        </fuel>
        <processtime>12</processtime>
        <nbatches>3</nbatches>
        <batchsize>19.91</batchsize>
        <commodity_production>
          <commodity>power</commodity>
          <capacity>90</capacity>
          <cost>90</cost>
        </commodity_production>
      </BatchReactor>
    </config>
    <incommodity>fresh_lwr_fuel</incommodity>
    <outcommodity>lwr_unf</outcommodity>
  </facility>

  <facility>
    <name>SFR</name>
    <lifetime>1</lifetime>
    <config>
      <BatchReactor>
        <fuel>
         <incommodity>fresh_sfr_fuel</incommodity>
         <inrecipe>fresh_sfr_fuel_recipe</inrecipe>
         <outcommodity>sfr_unf</outcommodity>
         <outrecipe>sfr_unf_recipe</outrecipe>
        </fuel>
        <processtime>12</processtime>
        <nbatches>4</nbatches> <!-- In cyclus, this must be an integer -->
        <batchsize>3.25875</batchsize> <!-- 3.95*(3.3/4) -->
        <commodity_production>
          <commodity>power</commodity>
          <capacity>30</capacity>
          <cost>30</cost>
        </commodity_production>
      </BatchReactor>
    </config>
    <incommodity>fresh_sfr_fuel</incommodity>
    <outcommodity>sfr_unf</outcommodity>
  </facility>

  <facility>
    <name>LWRWetStorage</name>
    <config>
      <CommodConverter>
        <commod_pair>
          <incommodity>lwr_unf</incommodity>
          <inrecipe>lwr_unf_recipe</inrecipe>
          <outcommodity>lwr_unf_cool</outcommodity>
          <outrecipe>lwr_unf_recipe</outrecipe>
        </commod_pair>
        <processtime>48</processtime>
        <commodity_production>
          <commodity>lwr_unf_recipe</commodity>
          <capacity>1000.0</capacity>
          <cost>0.0</cost>
        </commodity_production>
      </CommodConverter>
    </config>
    <incommodity>lwr_unf</incommodity>
    <outcommodity>lwr_unf_fuel</outcommodity>
  </facility>

  <facility>
    <name>SFRWetStorage</name>
    <config>
      <CommodConverter>
        <commod_pair>
          <incommodity>sfr_unf</incommodity>
          <inrecipe>sfr_unf_recipe</inrecipe>
          <outcommodity>sfr_unf_cool</outcommodity>
          <outrecipe>sfr_unf_recipe</outrecipe>
        </commod_pair>
        <processtime>12</processtime>
        <commodity_production>
          <commodity>sfr_unf_cool</commodity>
          <capacity>1000.0</capacity>
          <cost>0.0</cost>
        </commodity_production>
      </CommodConverter>
    </config>
    <incommodity>sfr_unf</incommodity>
    <outcommodity>sfr_unf_fuel</outcommodity>
  </facility>

  <facility>
    <name>LWRSeparations</name>
    <config>
      <SeparationsFac>
        <inpair>
          <incommodity>lwr_unf_cool</incommodity>
          <inrecipe>lwr_unf_recipe</inrecipe>
        </inpair>
        <outpair>
          <outcommodity>rep_lwr_tru</outcommodity>
          <outrecipe>rep_lwr_tru_recipe</outrecipe>
        </outpair>
        <outpair>
          <outcommodity>rep_lwr_u</outcommodity>
          <outrecipe>rep_lwr_u_recipe</outrecipe>
        </outpair>
        <processtime>0</processtime>
        <capacity>2000000</capacity>
        <commodity_production>
          <commodity>rep_lwr_u</commodity>
          <capacity>1000.0</capacity>
          <cost>0.0</cost>
        </commodity_production>
      </SeparationsFac>
    </config>
    <incommodity>lwr_unf</incommodity>
    <outcommodity>lwr_unf_fuel</outcommodity>
  </facility>

  <facility>
    <name>SFRSeparations</name>
    <config>
      <SeparationsFac>
        <inpair>
          <incommodity>sfr_unf_cool</incommodity>
          <inrecipe>sfr_unf_recipe</inrecipe>
        </inpair>
        <outpair>
          <outcommodity>rep_sfr_tru</outcommodity>
          <outrecipe>rep_sfr_tru_recipe</outrecipe>
        </outpair>
        <outpair>
          <outcommodity>rep_sfr_u</outcommodity>
          <outrecipe>rep_sfr_u_recipe</outrecipe>
        </outpair>
        <processtime>0</processtime>
        <capacity>2000000</capacity>
        <commodity_production>
          <commodity>rep_sfr_u</commodity>
          <capacity>1000.0</capacity>
          <cost>0.0</cost>
        </commodity_production>
      </SeparationsFac>
    </config>
    <incommodity>sfr_unf</incommodity>
    <outcommodity>sfr_unf_fuel</outcommodity>
  </facility>

  <facility>
    <name>OneFacility</name>
    <config>
      <Source>
        <out_commod>10.0</out_commod>
        <recipe_name>lwr_fresh_fuel</recipe_name>
      </Source>
    </config>
  </facility>

  <region>
    <name>SingleRegion</name>
    <config>
      <GrowthRegion>
        <commodity_name>power</commodity_name>
        <demand_types>
          <val>linear</val>
        </demand_types>
        <demand_params>
          <val>1 1</val>
        </demand_params>
        <demand_times>
          <val>1</val>
        </demand_times>
      </GrowthRegion>
    </config>
    <institution>
      <name>SingleInstitution</name>
      <initialfacilitylist>
        <entry>
          <prototype>Mine</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>Enrichment</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>LWRFuelFab</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>SFRFuelFab</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>LWRWetStorage</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>SFRWetStorage</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>LWRSeparations</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>SFRSeparations</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>Enrichment</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>LWR</prototype>
          <number>100</number>
        </entry>
      </initialfacilitylist>
      <config>
        <DecommInst>
          <target_fac>SFRFuelFab</target_fac>
          <num_to_build>-3</num_to_build>
          <target_commod>fresh_sfr_fuel</target_commod>
          <amt_req>10.0</amt_req>
          <prototypes>
            <val>Mine</val>
            <val>Enrichment</val>
            <val>LWRFuelFab</val>
            <val>SFRFuelFab</val>
            <val>LWR</val>
            <val>SFR</val>
            <val>LWRWetStorage</val>
            <val>SFRWetStorage</val>
            <val>LWRSeparations</val>
            <val>SFRSeparations</val>
          </prototypes>
        </DecommInst>
      </config>
    </institution>
  </region>


  <recipe>
