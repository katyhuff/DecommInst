<simulation>
  <control>
    <duration>10</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec><lib>cycamore</lib><name>Source</name></spec>
    <spec><lib>cycamore</lib><name>GrowthRegion</name></spec>
    <spec><path>decomm</path><lib>DecommInst</lib><name>DecommInst</name></spec>
  </archetypes>

  <facility>
    <name>OneFacility</name>
    <config>
      <Source/>
    </config>
  </facility>

  <region>
    <name>OneRegion</name>
    <config>
      <GrowthRegion/>
    </config>
    <institution>
      <name>OneInst</name>
      <initialfacilitylist>
        <entry>
          <prototype>OneFacility</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
      <config>
        <DecommInst/>
      </config>
    </institution>
  </region>

</simulation>
