<simulation>
  <control>
    <duration>10</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
  </control>

  <archetypes>
    <spec><path>decomm</path><lib>DecommFacility</lib><name>DecommFacility</name></spec>
    <spec><path>decomm</path><lib>DecommRegion</lib><name>DecommRegion</name></spec>
    <spec><path>decomm</path><lib>DecommInst</lib><name>DecommInst</name></spec>
  </archetypes>

  <facility>
    <name>OneFacility</name>
    <config>
      <DecommFacility />
    </config>
  </facility>

  <region>
    <name>OneRegion</name>
    <config>
      <DecommRegion />
    </config>
    <institution>
      <name>OneInst</name>
      <initialfacilitylist>
        <entry>
          <prototype>OneFacility</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
      <config>
        <DecommInst />
      </config>
    </institution>
  </region>

</simulation>
